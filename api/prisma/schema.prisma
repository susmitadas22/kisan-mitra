generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Image {
  id        String   @id @default(cuid())
  sub       String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  disease   String?
  cure      String?
  prevent   String?
  mimeType  String
  lat       Float
  lng       Float
}
